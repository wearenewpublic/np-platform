{
    "name": "np-platform-client",
    "version": "0.37.2",
    "license": "Apache-2.0",
    "scripts": {
        "demo": "expo start --web",
        "test": "jest",
        "testsummary": "jest 2>&1 | grep 'FAIL'",
        "coverage": "jest --coverage",
        "build": "babel . --out-dir dist --ignore node_modules,dist --source-maps",
        "prepublishOnly": "yarn build",
        "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore"
    },
    "exports": {
        "./component/*": "./dist/component/*.js",
        "./feature": "./dist/feature/index.js",
        "./feature/*": "./dist/feature/*.js",
        "./organizer/*": "./dist/organizer/*.js",
        "./structure": "./dist/structure/index.js",
        "./structure/*": "./dist/structure/*.js",
        "./system/*": "./dist/system/*.js",
        "./translations": "./dist/translations/index.js",
        "./util/*": "./dist/util/*.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wearenewpublic/np-platform"
    },
    "jest": {
        "preset": "jest-expo",
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/dist/"
        ],
        "modulePathIgnorePatterns": [
            "/dist/"
        ],
        "testEnvironment": "jsdom",
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "setupFilesAfterEnv": [
            "./util/setuptests.js"
        ],
        "snapshotSerializers": [
            "<rootDir>/util/snapshotserializer.js"
        ]
    },
    "peerDependencies": {
        "@carbon/icons-react": "^11.36.0",
        "expo": "^49.0.0",
        "expo-font": "~11.1.1",
        "expo-linking": "~4.0.1",
        "expo-status-bar": "~1.4.4",
        "react": "18.2.0",
        "react-dom": "^18.2.0",
        "react-native": "0.72.6",
        "react-native-web": "~0.19.6"
    },
    "dependencies": {
        "@babel/runtime": "^7.23.9",
        "@expo-google-fonts/ibm-plex-mono": "^0.2.3",
        "expo-linear-gradient": "~12.3.0",
        "firebase": "^9.22.2",
        "json5": "^2.2.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.23.4",
        "@babel/core": "^7.20.0",
        "@babel/preset-env": "^7.22.4",
        "@carbon/icons-react": "^11.36.0",
        "@expo-google-fonts/ibm-plex-sans": "^0.2.3",
        "@expo/webpack-config": "^19.0.0",
        "@testing-library/jest-native": "^5.4.3",
        "@testing-library/react": "^14.2.0",
        "eslint": "^8.56.0",
        "eslint-plugin-react": "^7.33.2",
        "expo": "^49.0.0",
        "expo-constants": "~14.4.2",
        "expo-font": "~11.4.0",
        "expo-linking": "~5.0.2",
        "expo-status-bar": "~1.6.0",
        "jest": "^29.7.0",
        "jest-expo": "~49.0.0",
        "react": ">=16.13.1",
        "react-dom": ">=16.13.1",
        "react-native": "0.72.6",
        "react-native-web": ">=0.18.11",
        "react-test-renderer": "^18.2.0"
    }
}
